{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "sourceMap": true,
    "outDir": ".dist",
    "baseUrl": ".",
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
  }
}